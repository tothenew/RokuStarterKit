<?xml version="1.0" encoding="UTF-8"?>
<component name="DialogUtility"
    extends="Group">

    <interface>
        <field id="enableProgressDialog"
            type="boolean"
            alwaysNotify="true"
            onChange="onProgressDialogEnabled" />
        <field id="disableProgressDialog"
            type="boolean"
            alwaysNotify="true"
            onChange="onProgressDialogDisabled" />
        <field id="isProgressDialogVisible"
            type="boolean"
            alwaysNotify="true" />

        <field id="enableErrorDialog"
            type="boolean"
            alwaysNotify="true"
            onChange="onErrorDialogEnabled" />
        <field id="disableErrorDialog"
            type="boolean"
            alwaysNotify="true"
            onChange="onErrorDialogDisabled" />
        <field id="isErrorDialogVisible"
            type="boolean"
            alwaysNotify="true" />

        <field id="errorTitle"
            type="String"
            alwaysNotify="true" />
        <field id="errorMessage"
            type="String"
            alwaysNotify="true" />
    </interface>

</component>
